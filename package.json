{
  "name": "rxjs-web-midi",
  "version": "1.0.0",
  "description": "RxJS bindings for Web MIDI API",
  "main": "",
  "scripts": {
    "watch:example": "watchify example/app.js -t babelify -o example/dist.js -v",
    "build:example": "browserify example/app.js -t babelify -o example/dist.js",
    "build": "browserify lib/rxjs-web-midi.js -t babelify -o dist/rxjs-web-midi.js",
    "test": "eslint lib/*.js test/*.js && mocha --compilers js:babel/register"
  },
  "author": "Arto Chydenius",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.8.23",
    "babelify": "^6.3.0",
    "browserify": "^11.2.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "mocha": "^2.3.3",
    "watchify": "^3.4.0"
  },
  "dependencies": {
    "rx": "^3.1.2"
  }
}
